{
    "name": "mup0c/phalyfusion",
    "description": "Utility for combining a bunch of php static code analyzers output.",
    "autoload": {
        "psr-4": { "Phalyfusion\\": "src/" }
    },
    "type": "project",
    "authors": [
        {
            "name": "Miroslav Koberskii",
            "email": "g4miros@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.1",
        "composer/composer": "^1.10",
        "nette/neon": "^3.0",
        "symfony/process": "^5.0",
        "symfony/console": "^5.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.2",
        "phan/phan": "^2.6",
        "vimeo/psalm": "3.9",
        "phpmd/phpmd": "^2.8",
        "taptima/php-cs-fixer": "dev-master"
    },
    "bin": [
        "phalyfusion"
    ],
    "config": {
        "bin-dir": "bin"
    }
}
